
<%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
<%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
<link href="https://use.fontawesome.com/releases/v5.0.6/css/all.css" rel="stylesheet">
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<%= csrf_meta_tags %>
<%= csp_meta_tag %>
<body class="h-100">
    <div class="container-fluid">
        <div class="row">
        <%= render partial: 'layouts/dashboard/sidebar' %>
        <main class="main-content col-lg-10 col-md-9 col-sm-12 p-0 offset-lg-2 offset-md-3">
            <div class="main-navbar sticky-top bg-white">
            <%= render partial: 'layouts/dashboard/navbar' %>
            </div>
            <div class="main-content-container container-fluid px-4">
            <%= yield %>
            </div>
            <%= render partial: 'layouts/dashboard/footer' %>
        </main>
        </div>
    </div>
    <%= javascript_include_tag 'jquery', 'data-turbolinks-track': 'realod'%>
    <script src="https://unpkg.com/shards-ui@latest/dist/js/shards.min.js" defer></script>
    <%= javascript_include_tag 'shards-dashboards.js', 'data-turbolinks-track': 'reload' %>
</body>